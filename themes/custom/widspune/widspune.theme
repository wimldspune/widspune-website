/**
 * Implements hook_page_attachments_alter().
 */
function widspune_page_attachments_alter(&$attachments) {
  if (\Drupal::service('path.matcher')->isFrontPage()) {
    $attachments['#attached']['library'][] = 'widspune/numscroller';
  }
}